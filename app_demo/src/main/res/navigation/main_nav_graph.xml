    android:id="@+id/main_nav_graph"
    app:startDestination="@id/demoFragment">

    <fragment
        android:id="@+id/demoFragment"
        android:name="com.usacheow.appdemo.DemoFragment"
        android:label="DemoFragment" >
        <action
            android:id="@+id/action_demoFragment_to_fontsFragment"
            app:destination="@id/fontsFragment" />
        <action
            android:id="@+id/action_demoFragment_to_buttonsFragment"
            app:destination="@id/buttonsFragment" />
        <action
            android:id="@+id/action_demoFragment_to_textInputsFragment"
            app:destination="@id/textInputsFragment" />
        <action
            android:id="@+id/action_demoFragment_to_actionTilesFragment"
            app:destination="@id/actionTilesFragment" />
        <action
            android:id="@+id/action_demoFragment_to_listTilesFragment"
            app:destination="@id/listTilesFragment" />
        <action
            android:id="@+id/action_demoFragment_to_tagListFragment"
            app:destination="@id/tagListFragment" />
        <action
            android:id="@+id/action_demoFragment_to_informationTilesFragment"
            app:destination="@id/informationTilesFragment" />
        <action
            android:id="@+id/action_demoFragment_to_errorMessageFragment"
            app:destination="@id/errorMessageFragment" />
        <action
            android:id="@+id/action_demoFragment_to_numPadFragment"
            app:destination="@id/numPadFragment" />
        <action
            android:id="@+id/action_demoFragment_to_exampleBottomDialogFragment"
            app:destination="@id/exampleBottomDialogFragment" />
        <action
            android:id="@+id/action_demoFragment_to_exampleModalFragment"
            app:destination="@id/exampleModalFragment" />

        <action
            android:id="@+id/action_demoFragment_to_onBoardingFragment"
            app:destination="@id/onBoardingFragment" />
