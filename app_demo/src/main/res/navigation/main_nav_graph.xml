    <fragment
        android:id="@+id/fontsFragment"
        android:name="com.usacheow.appdemo.catalog.FontsFragment"
        android:label="FontsFragment" />
    <fragment
        android:id="@+id/buttonsFragment"
        android:name="com.usacheow.appdemo.catalog.ButtonsFragment"
        android:label="ButtonsFragment" />
    <fragment
